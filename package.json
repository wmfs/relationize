{
  "name": "@wmfs/relationize",
  "version": "0.0.0-semantically-release",
  "description": "Takes a set of JSON Schema definitions and returns a relational database structure capable of storing compliant data.",
  "author": "West Midlands Fire Service",
  "homepage": "https://github.com/wmfs/relationize#readme",
  "keywords": [
    "tymly",
    "package",
    "json",
    "schema",
    "pg",
    "postgres"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wmfs/relationize.git"
  },
  "bugs": {
    "url": "https://github.com/wmfs/relationize/issues"
  },
  "main": "./lib/index.js",
  "dependencies": {
    "glob": "8.0.3",
    "jsonfile": "6.1.0",
    "lodash": "4.17.21",
    "precision": "1.0.1"
  },
  "devDependencies": {
    "chai": "4.3.6",
    "chai-subset": "1.6.0",
    "codecov": "3.8.3",
    "conventional-changelog-metahub": "4.0.1",
    "cz-conventional-changelog": "3.3.0",
    "mocha": "10.1.0",
    "nyc": "15.1.0",
    "semantic-release": "19.0.5",
    "standard": "17.0.0",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1"
  },
  "scripts": {
    "lint": "standard",
    "test": "nyc mocha",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "semantic-release": "semantic-release"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}

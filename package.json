{
  "name": "@wmfs/relationize",
  "version": "0.0.0-semantically-release",
  "description": "Takes a set of JSON Schema definitions and returns a relational database structure capable of storing compliant data.",
  "author": "West Midlands Fire Service",
  "homepage": "https://github.com/wmfs/relationize#readme",
  "keywords": [
    "tymly",
    "package",
    "json",
    "schema",
    "pg",
    "postgres"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wmfs/relationize.git"
  },
  "bugs": {
    "url": "https://github.com/wmfs/relationize/issues"
  },
  "main": "./lib/index.js",
  "dependencies": {
    "async": "2.6.1",
    "boom": "7.2.0",
    "debug": "3.1.0",
    "glob": "7.1.2",
    "jsonfile": "4.0.0",
    "lodash": "4.17.10",
    "precision": "1.0.1"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "chai-subset": "1.6.0",
    "codecov": "3.0.2",
    "cz-conventional-changelog": "2.1.0",
    "mocha": "5.2.0",
    "nyc": "12.0.2",
    "semantic-release": "^15.5.1",
    "standard": "11.0.1"
  },
  "scripts": {
    "lint": "standard",
    "test": "nyc mocha && standard",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "analyzeCommits": {
      "preset": "angular",
      "releaseRules": [
        {"type": "build", "scope":"deps", "release": "minor"},
        {"type": "build", "scope":"devDeps", "release": "patch"}
      ]
    }
  }
}
